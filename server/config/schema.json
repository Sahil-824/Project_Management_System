{
  "githubApis": {
    "fetchIssuesList": {
      "schema": {
        "url": "/github/fetchIssuesList",
        "body": {
          "type": "object",
          "properties": {},
          "required": []
        }
      }
    },
    "fetchCommitHistory": {
      "schema": {
        "url": "/github/fetchCommitHistory",
        "body": {
          "type": "object",
          "properties": {},
          "required": []
        }
      }
    },
    "fetchRepoLinks": {
      "schema": {
        "url": "/github/fetchRepoLinks",
        "body": {
          "type": "object",
          "properties": {},
          "required": []
        }
      }
    },
    "fetchPullRequest": {
      "schema": {
        "url": "/github/fetchPullRequest",
        "body": {
          "type": "object",
          "properties": {},
          "required": []
        }
      }
    }
  },
  "emailService": {
    "schema": {
      "url": "/api/sendEmail",
      "body": {
        "type": "object",
        "properties": {
          "emailId": {
            "type": "string"
          }
        },
        "required": ["emailId"]
      }
    }
  },
  "clientLogin": {
    "schema": {
      "url": "/api/clientLogin",
      "body": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": ["email", "password"]
      }
    }
  },
  "clientSignup": {
    "schema": {
      "url": "/api/clientSignup",
      "body": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": ["email", "password"]
      }
    }
  },
  "projectRegistration": {
    "schema": {
      "url": "/api/projectRegistration",
      "body": {
        "type": "object",
        "properties": {
          "projectName": {
            "type": "string"
          },
          "projectDescription": {
            "type": "string"
          },
          "githubRepoUrl": {
            "type": "string"
          },
          "members": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "timeLines": {
            "type": "string"
          },
          "startDate": {
            "type": "string"
          },
          "projectStatus": {
            "type": "string",
            "enum": ["Not Started", "In Progress", "Completed"]
          },
          "comments": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["projectName", "githubRepoUrl"]
      }
    }
  },
  "addProjectMembers": {
    "schema": {
      "url": "/api/addProjectMembers",
      "body": {
        "type": "object",
        "properties": {
          "projectId": {
            "type": "string"
          },
          "member": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["projectId", "member"]
      }
    }
  }
}
